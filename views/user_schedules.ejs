<%- include('partials/header') %>
    <h1>Schedule/s of <%= user.firstname %> <%= user.lastname %>:  </h1>
    <table border="1px solid black">
        <thead>
            <tr>
                <th>Day</th>
                <th>Start Time</th>
                <th>End time</th>
            </tr>
        </thead>
        <tbody>
            <% for(let schedule of schedules) { %>
                <tr>
                    <td><%= schedule.day %></td>
                    <td><%= schedule.start_at %></td>
                    <td><%= schedule.end_at %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
<%- include('partials/footer') %>